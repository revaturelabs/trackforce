<!-- Use locally to run these tests -->
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="1" verbose="10" name="Surefire suite">
	<test name="Resources tests" preserve-order="true">
		<groups>
			<run>
				<!-- <include name="GET"  /> -->
				<exclude name="PUT"  />
				<exclude name="POST"  />
				<exclude name="DELETE"  />
			</run>
		</groups>
		<packages>
			<package name="com.revature.test.resources" />
		</packages>
	</test>
</suite> <!-- Surefire suite --> 